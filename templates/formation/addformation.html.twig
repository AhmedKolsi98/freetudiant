{% extends 'base.html.twig' %}
{% block content %}
{% endblock %}

{% block body %}

    <div class="tab-content" id="formationtab">
        <div class="tab-pane fade active show" id="formation-detail" role="tabpanel" aria-labelledby="formation-detail-tab">
            <section class="space-ptb">
                <div class="container">
                    {{ form_start(form) }}
                    {{ form_row(form.title,{ 'attr':{'placeholder':" Title" , 'class':"form-control"}}) }}
                    {{ form_row(form.description,{ 'attr':{'placeholder':"Description" ,'class':"form-control"}}) }}
                    {{ form_row(form.date) }}
                    {{ form_row(form.localisation,{ 'attr':{'placeholder':"Localisation" ,'class':"form-control"}}) }}
                    {{ form_row(form.Add,{ 'attr':{ 'class':"btn btn-primary"}}) }}
                    {{ form_end(form) }}
                </div>
            </section>
        </div>
    </div>







{% endblock %}




